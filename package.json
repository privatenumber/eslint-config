{
	"name": "@pvtnbr/eslint-config",
	"version": "0.0.0-semantic-release",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"repository": "privatenumber/eslint-config",
	"funding": "https://github.com/privatenumber/eslint-config?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			}
		},
		"./___pkg-maps-resolver": "./dist/resolvers/pkg-maps-resolver.cjs"
	},
	"imports": {
		"#pvtnbr": {
			"types": "./src/index.ts",
			"development": "./src/index.ts",
			"default": "./dist/index.mjs"
		},
		"#pkg-maps-resolver": {
			"types": "./src/resolvers/pkg-maps-resolver.ts",
			"development": "./src/resolvers/pkg-maps-resolver.ts",
			"default": "./dist/resolvers/pkg-maps-resolver.cjs"
		}
	},
	"bin": "./dist/cli.mjs",
	"scripts": {
		"build": "rm -rf dist; pkgroll --export-condition development",
		"type-check": "tsc --noEmit",
		"lint": "NODE_OPTIONS='--import tsx --conditions=development' eslint .",
		"dev": "tsx watch --conditions=development tests",
		"test": "tsx tests",
		"prepack": "pnpm build && clean-pkg-json"
	},
	"peerDependencies": {
		"eslint": "^8.0.0"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
		"@eslint/js": "^8.56.0",
		"@stylistic/eslint-plugin": "^1.5.1",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"cleye": "^1.3.2",
		"confusing-browser-globals": "^1.0.11",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsonc": "^2.11.1",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.4.0",
		"eslint-plugin-no-use-extend-native": "^0.5.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-regexp": "^2.1.2",
		"eslint-plugin-unicorn": "^49.0.0",
		"eslint-plugin-vue": "^9.19.2",
		"find-up-simple": "^1.0.0",
		"get-conditions": "^1.0.0",
		"get-tsconfig": "^4.7.2",
		"globals": "^13.24.0",
		"read-package-up": "^11.0.0",
		"resolve-pkg-maps": "^1.0.0",
		"vue-eslint-parser": "^9.3.2"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/confusing-browser-globals": "^1.0.3",
		"@types/eslint": "^8.44.9",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.45",
		"clean-pkg-json": "^1.2.0",
		"eslint": "^8.56.0",
		"eslint-plugin-prefer-arrow-functions": "^3.2.4",
		"execa": "^8.0.1",
		"manten": "^1.2.0",
		"pkgroll": "^2.0.1",
		"react": "^18.2.0",
		"tsx": "^4.6.2",
		"typescript": "^5.3.3"
	}
}
