{
	"name": "@pvtnbr/eslint-config",
	"version": "0.0.0-semantic-release",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"repository": "privatenumber/eslint-config",
	"funding": "https://github.com/privatenumber/eslint-config?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./node": "./dist/node.js"
	},
	"scripts": {
		"build": "rm -rf dist; esbuild src/**/*.ts --format=cjs --outdir=dist",
		"type-check": "tsc --noEmit",
		"lint": "NODE_OPTIONS='--import tsx' eslint --cache .",
		"test": "pnpm build && tsx tests",
		"dev:build": "pnpm build --watch",
		"dev:test": "tsx watch tests",
		"dev": "run-p 'dev:*'",
		"prepack": "pnpm build && clean-pkg-json"
	},
	"peerDependencies": {
		"eslint": "^8.0.0"
	},
	"dependencies": {
		"@eslint/js": "^8.55.0",
		"@stylistic/eslint-plugin": "^1.4.1",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"confusing-browser-globals": "^1.0.11",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jsonc": "^2.10.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.3.1",
		"eslint-plugin-no-use-extend-native": "^0.5.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-regexp": "^2.1.1",
		"eslint-plugin-unicorn": "^49.0.0",
		"eslint-plugin-vue": "^9.18.1",
		"get-tsconfig": "^4.7.2",
		"globals": "^13.23.0"
	},
	"devDependencies": {
		"@types/confusing-browser-globals": "^1.0.3",
		"@types/eslint": "^8.44.7",
		"@types/node": "^20.10.0",
		"@types/react": "^18.2.38",
		"clean-pkg-json": "^1.2.0",
		"esbuild": "^0.19.8",
		"eslint": "^8.54.0",
		"eslint-define-config": "^2.0.0",
		"jsonc-eslint-parser": "^2.4.0",
		"manten": "^1.2.0",
		"npm-run-all": "^4.1.5",
		"react": "^18.2.0",
		"tsx": "^4.5.0",
		"typescript": "^5.3.2"
	}
}
