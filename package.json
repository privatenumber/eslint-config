  "funding": "https://github.com/privatenumber/20x?sponsor=1",
  "author": {
    "name": "Hiroki Osame",
    "email": "hiroki.osame@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "bin": {
    "lint": "./dist/cli/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./___pkg-maps-resolver": "./dist/resolvers/pkg-maps-resolver.cjs"
  },
  "imports": {
    "#pvtnbr": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "#pkg-maps-resolver": {
      "types": "./src/resolvers/pkg-maps-resolver.ts",
      "development": "./src/resolvers/pkg-maps-resolver.ts",
      "default": "./dist/resolvers/pkg-maps-resolver.cjs"
    },
    "#cli": {
      "development": "./src/cli/index.ts",
      "default": "./dist/cli/index.mjs"
    }
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cleye": "^1.3.2",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.5.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-vue": "^9.25.0",
    "eslint-plugin-yml": "^1.14.0",
    "execa": "^8.0.1",
    "get-conditions": "^1.0.0",
    "get-tsconfig": "^4.7.3",
    "globals": "^15.0.0",
    "resolve-pkg-maps": "^1.0.0",
    "tsx": "^4.7.2",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  }
}