{
	"name": "@pvtnbr/eslint-config",
	"version": "0.0.0-semantic-release",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"repository": "privatenumber/eslint-config",
	"funding": "https://github.com/privatenumber/eslint-config?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"scripts": {
		"build": "esbuild src/**/*.ts --format=cjs --outdir=dist",
		"type-check": "tsc --noEmit",
		"lint": "eslint --cache .",
		"test": "pnpm build && tsx tests",
		"dev:build": "pnpm build --watch",
		"dev:test": "tsx watch tests",
		"dev": "run-p 'dev:*'",
		"prepack": "pnpm build && clean-pkg-json"
	},
	"engines": {
		"node": ">=16.18.0"
	},
	"peerDependencies": {
		"eslint": "^7.15.0 || ^8.0.0"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"confusing-browser-globals": "^1.0.11",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsonc": "^2.5.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-n": "^15.5.1",
		"eslint-plugin-no-use-extend-native": "^0.5.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-regexp": "^1.10.0",
		"eslint-plugin-unicorn": "^44.0.2",
		"eslint-plugin-vue": "^9.7.0",
		"get-tsconfig": "^4.2.0"
	},
	"devDependencies": {
		"@types/confusing-browser-globals": "^1.0.0",
		"@types/eslint": "^8.4.7",
		"@types/node": "^17.0.45",
		"@types/react": "^18.0.25",
		"clean-pkg-json": "^1.2.0",
		"esbuild": "^0.15.13",
		"eslint": "^8.27.0",
		"manten": "^0.5.0",
		"npm-run-all": "^4.1.5",
		"react": "^17.0.2",
		"tsx": "^3.12.1",
		"typescript": "^4.8.4"
	},
	"eslintConfig": {
		"extends": ".",
		"ignorePatterns": [
			"tests/*/fixtures"
		]
	}
}
